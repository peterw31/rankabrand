{
    "manifest_version": 2,
    "name": "Rank a brand",
    "description": "gets the rank a brand of the selected word",
    "version": "1.0",
    "background": { "scripts": [ "js/background.js" ] },
    "minimum_chrome_version": "6.0.0.0",
    "permissions": [
        "storage",
        "tabs",
        "contextMenus",
        "http://*/*",
        "https://*/*"
    ],
    "icons": {
        "16": "img/icon.png",
        "48": "img/icon.png",
        "128": "img/icon.png"
    },
    //compat for old chrome versions
    "options_page": "options.html",
    "options_ui": {
        "page": "options.html",
        "chrome_style": true
  }

}